# This is the Unneeded PM cleanup script script.

use strict;
use Config;

my @files = ("Edit.pm");

foreach
 my $file(@files) {
	$file = "$Config{installsitelib}/$file";
	if (-e $file) {
		print "Deleting $file\n";
		unlink ($file);
	}
 }
 print "Complete\n";